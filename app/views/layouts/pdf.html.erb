<! DOCTYPE html>
<html>
  <head>
    <title> PDF </title>
    <meta charset="utf-8">
    <%= wicked_pdf_stylesheet_pack_tag("application") %>
  </head>
  <body>
    <div class='container'>
      <div class="row">
        <%= image_tag(embed_remote_image("#{Rails.root}/public#{@enterprise.pictures.first.image.small_thumb.url}",'image/'), alt:'Logo') if @enterprise.pictures.first.image? %>
      </div>
      <%= yield %>
    </div>
  </body>
</html>